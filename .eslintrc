{
  "extends": [
    "plugin:svelte/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "extraFileExtensions": [
      ".svelte"
    ]
  },
  "overrides": [
    {
      "files": [
        "*.svelte"
      ],
      "parser": "svelte-eslint-parser",
      "parserOptions": {
        "parser": "@typescript-eslint/parser"
      },
      "rules": {
        "no-console": "error",
        "semi": [
          "warn",
          "never"
        ],
        "no-multiple-empty-lines": [
          "warn",
          {
            "max": 1,
            "maxBOF": 0,
            "maxEOF": 0
          }
        ]
      }
    }
  ],
  "rules": {
    "no-console": "error",
    "semi": [
      "warn",
      "never"
    ],
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1,
        "maxBOF": 0,
        "maxEOF": 0
      }
    ]
  }
}